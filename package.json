{
  "name": "@roehrt/baahn-cli",
  "version": "1.0.2",
  "description": "A simple CLI wrapper for baahn",
  "main": "dist/index.js",
  "author": "Tobias Roehr",
  "license": "ISC",
  "keywords": [
    "deutsche bahn",
    "cli",
    "transit",
    "germany"
  ],
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .ts .",
    "build": "tsc && printf '%s\\n%s\\n' \"#!/usr/bin/env node\n\" \"$(cat dist/index.js)\" > dist/index.js",
    "prepare": "npm run build"
  },
  "repository": "git://github.com/roehrt/baahn-cli.git",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@roehrt/baahn": "^1.0.10",
    "chalk": "^4.1.0",
    "hafas-client": "^5.22.2",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "inquirer-datepicker": "^2.0.0",
    "node-emoji": "^1.10.0",
    "ora": "^5.1.0"
  },
  "devDependencies": {
    "@types/hafas-client": "^5.22.0",
    "@types/inquirer": "^8.2.0",
    "@types/inquirer-autocomplete-prompt": "^1.3.3",
    "@types/node": "^17.0.18",
    "@types/node-emoji": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.5.5"
  },
  "bin": {
    "baahn": "dist/index.js"
  }
}
